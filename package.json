{
  "name": "biquad-fit",
  "version": "1.0.0",
  "description": "Compute optimal parametric EQ filter parameters to match a measured frequency response to a target curve. Reimplements the core algorithm from AutoEq. Zero dependencies.",
  "type": "module",
  "homepage": "https://github.com/CopperLoom/biquad-fit",
  "repository": {
    "type": "git",
    "url": "https://github.com/CopperLoom/biquad-fit.git"
  },
  "keywords": [
    "headphone",
    "audio",
    "eq",
    "equalization",
    "autoeq",
    "biquad",
    "parametric-eq",
    "peq",
    "frequency-response",
    "tuning",
    "filter",
    "dsp",
    "iem"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "prepare": "npm run build",
    "build": "esbuild src/index.js --bundle --format=esm --platform=neutral --outfile=dist/index.js && esbuild src/index.js --bundle --format=cjs --platform=neutral --outfile=dist/index.cjs && tsc",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "devDependencies": {
    "esbuild": "^0.27.3",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  },
  "author": "CopperLoom",
  "license": "MIT"
}
